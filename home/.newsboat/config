browser "google-chrome-stable"
articlelist-format "%4i %f %D %?T?|%-17T| ?%t"
auto-reload "yes"

bind-key k up
bind-key j down
bind-key h prev-feed
bind-key l next-feed
bind-key M mark-feed-read
bind-key m toggle-article-read
bind-key x toggle-show-read-feeds
bind-key ^U pageup
bind-key ^D pagedown
